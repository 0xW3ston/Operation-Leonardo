<div class="row w-75 ml-5 mt-5">
    <div class="col-sm"> 
        <a >
            <h3>Cours gratuits </h3>
            <% courses.forEach((f) =>{ %>
                <% if(f.payant === 0){ %>
                    <p><a href="/coursDetails/<%= f.id %>"><%= f.titre %></a></p>
            <% }}) %>
        </a>
    </div>
    <div class="col-sm"> 
        <a >
            <h3>Cours payants </h3>
            <% courses.forEach((f) =>{ %>
                <% if(f.payant === 1){ %>
                    <p><a href="/coursDetails/<%= f.id %>"><%= f.titre %></a></p>
            <% }}) %>
        </a>
    </div>
</div>
